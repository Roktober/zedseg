{
  "device": 0,
  "model": "unet512",
  "generator": {
    "type": "combo",
    "resolution": [320, 320],
    "items": [
      {
        "type": "png",
        "batch": 2
      },
      {
        "type": "h5",
        "batch": 2,
        "classes": [0, 1]
      }
    ]
  },
  "optimizer": {
    "type": "adam",
    "lr": 1e-4
  },
  "train": {
    "with_gui": true,
    "epoch_images": 64
  }
}
